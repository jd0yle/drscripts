gosub printSpellInfo %name





byField:
    gosub getFieldIndex

    eval numSpells count("%spells", "|")
    var index 0

    byFieldLoop:
        var details %spell.%spells(%index)

        if ("%details(%fieldIndex)" = "%fieldValue") then {
            #gosub printSpell %details
            var printIndexes %printIndexes|%index
            #goto done
        }
        math index add 1
        if (%index > %numSpells) then goto donePrint
        goto byFieldLoop


getFieldIndex:
    eval numFields count("%fields", "|")
    var fieldIndex 0

    getFieldIndexLoop:
        if ("%fields(%fieldIndex)" = "%fieldName") then return
        math fieldIndex add 1
        if (%fieldIndex > %numFields) then goto doneNoField
        goto getFieldIndexLoop



printSpell:
	eval itemLength count("%details", "|")
	var printSpellIndex 0
	printSpellLoop:
	    echo %fields(%printSpellIndex): %details(%printSpellIndex)
	    math printSpellIndex add 1
	    if (%printSpellIndex > %itemLength) then goto done
	    goto printSpellLoop


doneNoField:
    echo COuld not find field %fieldName
    exit

donePrint:
    var index 0
    eval numItems count("%printIndexes", "|")
    var msg
    donePrintLoop:
        var printSpellIndex %printIndexes(%index)
        var spellDetails %spell.%spells(%printSpellIndex)
        var msg %msg, %spellDetails(0)
        math index add 1
        if (%index > %numItems) then goto donePrintEcho
        goto donePrintLoop


donePrintEcho:
done:
	pause .1
	put #parse SPELLS DONE
	exit
    exit


# Sets up the detail properties for each spell
# ex: var spell.cv.abbr cv
#     var spell.cv.prepmin 1
#     var spell.cv.skill augmentation
populateDetails:
	var spellIndex 0
	eval numSpells count("%spells", "|")

	populateDetailsLoop:

	    var spellAbbr %spells(%spellIndex)
	    var spellDetails %spell.%spellAbbr
	    var detailIndex 0
	    eval detailLength count("%fields", "|")

	    populateSpellLoop:
	        var fieldName %fields(%detailIndex)
	        var spell.%spellAbbr.%fieldName %spellDetails(%detailIndex)
	        math detailIndex add 1
	        if (%detailIndex <= %detailLength) then goto populateSpellLoop

	    math spellIndex add 1
	    if (%spellIndex <= %numSpells) then goto populateDetailsLoop

    return



printSpellInfo:
	var spellAbbr $1
	echo
	echo Name: %spell.%spellAbbr.name      Abbr.: %spell.%spellAbbr.abbr
	echo Guild: %spell.%spellAbbr.guild      Spellbook: %spell.%spellAbbr.spellbook      Mana: %spell.%spellAbbr.mana      Slots: %spell.%spellAbbr.slots
	echo Difficulty: %spell.%spellAbbr.difficulty      Type: %spell.%spellAbbr.type      Skill: %spell.%spellAbbr.skill
	echo Offense Contest: %spell.%spellAbbr.contestoffense      Defense Contest: %spell.%spellAbbr.contestdefense      Notes: %spell.%spellAbbr.notes
	echo Prep Min:%spell.%spellAbbr.prepmin Max: %spell.%spellAbbr.prepmax   -   Skill Min: %spell.%spellAbbr.skillmin Max: %spell.%spellAbbr.skillmax   -   Duration Min: %spell.%spellAbbr.durationmin Max: %spell.%spellAbbr.durationmax
	echo Effect: %spell.%spellAbbr.effect
	echo
	gosub parseSpellInfo
	return
	goto done


parseSpellInfo:
	var index 0
	eval len count("%fields", "|")

	parseSpellInfoLoop:
		if (%index > %len) then return
		#echo SPELLINFO %fields(%index) %spell.%spellAbbr.%fields(%index)
		put #parse SPELLINFO %fields(%index) %spell.%spellAbbr.%fields(%index)
		math index add 1
		goto parseSpellInfoLoop


spells.end:
	pause .1
	put #parse SPELLS DONE
	exit